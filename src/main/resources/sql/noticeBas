CREATE TABLE `notice_bas` (
  `NOTICE_SNO` int NOT NULL AUTO_INCREMENT COMMENT '공지사항 일련번호',
  `TITLE` varchar(255) NOT NULL COMMENT '제목',
  `CONTENT` varchar(5000) DEFAULT NULL COMMENT '본문',
  `CREAT_USER` varchar(15) NOT NULL COMMENT '작성자',
  `CREAT_DT` datetime(6) NOT NULL COMMENT '등록일',
  `EXPSR_CNT` int NOT NULL DEFAULT '0' COMMENT '조회수',
  `USE_YN` varchar(1) NOT NULL DEFAULT 'Y' COMMENT '사용여부',
  `IMP_YN` varchar(1) NOT NULL DEFAULT 'N' COMMENT '주요공지노출',
  PRIMARY KEY (`NOTICE_SNO`)
) ENGINE=InnoDB AUTO_INCREMENT=104 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci COMMENT='공지사항';