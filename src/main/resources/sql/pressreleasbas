CREATE TABLE `press_releas_bas` (
  `PRRE_SNO` int NOT NULL AUTO_INCREMENT COMMENT '보도자료 일련번호',
  `TITLE` varchar(255) NOT NULL COMMENT '제목',
  `SUB_TITLE` varchar(500) NOT NULL COMMENT '부제목',
  `CREAT_DT` datetime(6) NOT NULL COMMENT '등록일',
  `CREAT_USER` varchar(15) NOT NULL COMMENT '작성자',
  `EXPSR_CNT` int NOT NULL DEFAULT '0' COMMENT '조회수',
  `IMG_URL` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL COMMENT '이미지URL',
  PRIMARY KEY (`PRRE_SNO`)
) ENGINE=InnoDB AUTO_INCREMENT=10 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci COMMENT='보도자료';