CREATE TABLE `promotion_bas` (
  `PRO_SNO` int NOT NULL AUTO_INCREMENT COMMENT '홍보영상 일련번호',
  `TITLE` varchar(255) NOT NULL COMMENT '제목',
  `SUB_TITLE` varchar(500) NOT NULL COMMENT '부제목',
  `CREAT_DT` datetime(6) NOT NULL COMMENT '등록일',
  `LINK_URL` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL COMMENT '링크URL',
  `IMG_URL` varchar(255) CHARACTER SET utf8mb4 COLLATE utf8mb4_0900_ai_ci DEFAULT NULL COMMENT '이미지URL',
  PRIMARY KEY (`PRO_SNO`)
) ENGINE=InnoDB AUTO_INCREMENT=4 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci COMMENT='홍보영상';